import "lang.spk";

let a = 0;
const pi = 3.1415926;

class Point
{
	new (x,y){
		self.x = x;
		self.y = y;
	}
	fn lengthSquared(){
		return (self.x*self.x + self.y *self.y);
	}
}
class Foo : Point
{
	new (x,y){
		Point.new(x,y);
	}
	fn length(){
		return Math.sqrt(self.lengthSquared());
	}
}
fn compute_pi(){
	let x = 0;
	let dx = 0.00000001;
	let s = 0;
	while x < 1 {
		s = s + dx/(x*x+1);
		x = x + dx;
	}
	return 4*s;
}

print(compute_pi());
let p = new Foo(3,4);
print(p);
print(p.length());